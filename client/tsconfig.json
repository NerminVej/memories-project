{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "lib": ["es6", "dom"],
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "baseUrl": "./src",
    "paths": {
      "*": ["*", "src/*"],
      "images/*": ["src/images/*"]
      }
    },
    "include": ["src", "src/images", "custom.d.ts"],
      "exclude": []
  }
  